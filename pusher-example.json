{
    "name": "my-app",
    "primaryDomain": {
        "wwwUsed": true,
        "wwwIsPrimary": false, 
        "fqRootDns": "my-app.io"
    },
    "secondaryDomains": {
        "tags": {
            "enabled": true,
            "dnsOffset": "tags",
            "whitelist": "$[regex]^",
            "blacklist": null
        },
        "releases": {
            "enabled": true,
            "dnsOffset": "tags",
            "whitelist": "$[regex]^",
            "blacklist": null
        },
        "commits": {
            "enabled": true,
            "dnsOffset": "tags",
            "whitelist": "$[regex]^",
            "blacklist": null
        }
    },
    "pipelines": {
        "init": [
            "project-detect",
            "user-interact",
            "save-config"
        ],
        "aws-init": [
            "iam-inspect",
            "iam-config",
            "s3-inspect",
            "s3-config-primary",
            "s3-config-secondary",
            "cloudfront-inspect",
            "cloudfront-config",
            "route53-inspect",
            "route53-config"
        ]
    }
}